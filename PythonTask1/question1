num=int(input("Enter number of strings to be inputted: "))
lst=[]
print("Enter text: ")
for i in range(0,num):
    lst_inp=input("")
    lst.append(lst_inp)

print(lst)

dict_cnt={}
for i in range(len(lst)):
    
    for a in lst[i]:

      if a in dict_cnt:
        dict_cnt[a]+=1
      else:
        dict_cnt[a]=1
print(dict_cnt)     